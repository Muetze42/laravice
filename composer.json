{
    "name": "norman-huth/lara-vice",
    "type": "project",
    "description": "The skeleton application for the Laravel framework. Created with norman-huth/advanced-laravel-installer.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "authors": [
        {
            "name": "Norman Huth",
            "homepage": "https://huth.it"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-exif": "*",
        "ext-gd": "*",
        "ext-imagick": "*",
        "barryvdh/laravel-dompdf": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "norman-huth/gd-text": "^1.0",
        "spatie/browsershot": "^4.0",
        "spatie/image": "^3.3",
        "spatie/laravel-sitemap": "^7.1",
        "symfony/yaml": "^7.0",
        "twig/twig": "^3.8"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "laravel/dusk": "^7.12",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "norman-huth/helpers-collection-laravel": "^1.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "functions/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan app:config"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "phpmd": [
            "phpmd app,database,config,routes text phpmd.xml"
        ],
        "pint": [
            "./vendor/bin/pint"
        ],
        "code-quality": [
            "./vendor/bin/pint",
            "phpmd app,database,config,routes text phpmd.xml"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
